            android:name="br.ufpe.cin.ess.saald.ManagerActivity"
            android:label="@string/title_activity_manager" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
